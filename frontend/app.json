{
  "expo": {
    "name": "FloodMap",
    "slug": "flood-map",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "floodmap",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0E17"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.floodmap.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0A0E17"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.floodmap.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["@rnmapbox/maps", {}],

      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map for flood monitoring."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9dc12811-462e-4417-95f6-e7f87d0a31c4"
      }
    }
  }
}
